<!-- index.html.erb -->

<div class="container">
  <div class="row">
    <div class="col-12 text-center">
      <h1>予約画面</h1>
      <p>3ヶ月先まで予約することができます。</p>
    </div>
    <div class="col-12 mt-3">
     @.each do |workshop|
    <%current_user.reservations.each do|reservation|%>
    <p> <%=reservation.name%></p>
    <p><%=reservation.date_time_1%></p>
    <p><%=reservation.date_time_2%></p>
    <p><%=reservation.date_time_3%></p>
    <p><%=reservation.participant%>名参加希望</p>
    <p><%=reservation.check_box%></p>
      <%=link_to "編集",users_reservation_edit_path(params[:workshop_id])%>
      <%=link_to "削除",#%>
    <% end %>  
      
    </div>
  </div>
</div>